{
  "name": "teamachine-server",
  "version": "1.0.0",
  "description": "Server for a perfect tea maker",
  "scripts": {
    "start": "mkdir -p dist/js && ./node_modules/.bin/browserify client/app.js --debug -t babelify -o dist/js/app.js && ./bin/www",
    "watch": "mkdir -p dist/js && ./node_modules/.bin/watchify client/app.js --debug -t babelify -o dist/js/app.js",
    "test": "./node_modules/.bin/jsxhint client/* server/* --reporter ./node_modules/jshint-stylish/stylish.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bilalq/TeaMachine-Server.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bilalq/TeaMachine-Server/issues"
  },
  "homepage": "https://github.com/bilalq/TeaMachine-Server",
  "dependencies": {
    "babelify": "^5.0.3",
    "bluebird": "^2.9.12",
    "body-parser": "^1.12.0",
    "browserify": "^9.0.3",
    "chart.js": "^1.0.1",
    "cookie-parser": "^1.3.4",
    "debug": "^2.1.1",
    "express": "^4.11.2",
    "jade": "^1.9.2",
    "lodash": "^3.3.0",
    "morgan": "^1.5.1",
    "node-sass": "^2.0.1",
    "node-sass-middleware": "^0.5.0",
    "react": "^0.12.2",
    "react-chartjs": "^0.4.0",
    "request": "^2.53.0",
    "request-promise": "^0.4.0",
    "serve-favicon": "^2.2.0",
    "watchify": "^2.4.0"
  },
  "devDependencies": {
    "jshint-stylish": "^1.0.0",
    "jsxhint": "^0.12.0",
    "nodemon": "^1.3.7"
  }
}
